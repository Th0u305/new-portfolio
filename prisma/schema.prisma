generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model image {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model projectsData {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  description String
  git         String
  image       String
  link        String
  technology  String[]
  title       String
  type        String
}
